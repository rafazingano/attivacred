<script>

   $(document).ready(function(){
    $('#step2').click(function(){
        $.ajax({
            url : 'http://temporario.web7073.uni5.net/AtivaCred/Imovel/dados',
            type : 'POST',
            data : 'valor=' + $('#valor').val() +'&nome=' + $('#nome').val(),
			
			
            success: function(data){
				
                $("#recebe").load("http://temporario.web7073.uni5.net/AtivaCred/welcome/imovel/step3")	
            }
        });
    });
})
</script>
